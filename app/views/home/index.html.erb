<% if @stock %>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><strong><%= @stock.name %></strong></h3>
  </div>
  <div class="panel-body">
    
  <div class="col-md-4">
   <h1>$<%= @stock.l %></h1><br/>
   
   <strong>Symbol:</strong> <%= @stock.symbol %><br/>
   <strong>Earnings Per Share: </strong><%= @stock.eps %><br/>
   <strong>Price/Earning Ratio: </strong><%= @stock.pe %><br/>
   <strong>Market Cap: </strong><%= @stock.mc %><br/>
   <strong>Stock Open: </strong><%= @stock.op %><br/>
   <strong>Year High: </strong><%= @stock.hi52 %><br/>
   <strong>Year Low: </strong><%= @stock.lo52 %><br/>
   </div>
  

<div class="col-md-8"><br/><br/><br/><br/><br/>
<p align="justify"><%= @stock.summary[0]['overview'] %></p>
</div>

<% elsif @nothing %>
<%= @nothing %>

<% end %>

<% if @error %>
    <%= @error %>
<% end %>
